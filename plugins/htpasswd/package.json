{
  "name": "verdaccio-htpasswd",
  "version": "10.5.3",
  "description": "htpasswd auth plugin for Verdaccio",
  "keywords": [
    "verdaccio",
    "plugin",
    "auth",
    "htpasswd"
  ],
  "author": "Ayush Sharma <ayush.aceit@gmail.com>",
  "license": "MIT",
  "homepage": "https://verdaccio.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/verdaccio/monorepo",
    "directory": "plugins/htpasswd"
  },
  "bugs": {
    "url": "https://github.com/verdaccio/monorepo/issues"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "engines": {
    "node": ">=12"
  },
  "dependencies": {
    "@verdaccio/file-locking": "10.3.0",
    "apache-md5": "1.1.8",
    "bcryptjs": "2.4.3",
    "http-errors": "2.0.0",
    "unix-crypt-td-js": "1.1.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "2.4.2",
    "@verdaccio/legacy-types": "workspace:1.0.1",
    "mockdate": "3.0.5"
  },
  "scripts": {
    "clean": "rimraf ./build",
    "test": "cross-env NODE_ENV=test BABEL_ENV=test jest",
    "type-check": "tsc --noEmit -p tsconfig.build.json",
    "build:js": "babel src/ --out-dir lib/ --copy-files --extensions \".ts,.tsx\" --source-maps",
    "watch": "pnpm build:js -- --watch",
    "build": "pnpm run build:js"
  },
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/verdaccio"
  }
}
